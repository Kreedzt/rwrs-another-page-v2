{
	"name": "rwrs-another-page-v2",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "node --experimental-strip-types scripts/build-sveltekit.ts",
		"build:cdn": "node --experimental-strip-types scripts/build-cdn-sveltekit.ts",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"coverage": "vitest run --coverage",
		"generate-icons": "tsx scripts/generate-icons.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.2",
		"@fontsource/fira-mono": "^5.2.7",
		"@neoconfetti/svelte": "^2.2.2",
		"@playwright/test": "^1.57.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@sveltejs/vite-plugin-svelte-inspector": "^5.0.1",
		"@tailwindcss/forms": "^0.5.11",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^25.0.3",
		"@vitest/coverage-v8": "^4.0.16",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"jsdom": "^26.1.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"pwa-asset-generator": "^8.1.2",
		"svelte": "^5.46.1",
		"svelte-check": "^4.3.5",
		"tailwindcss": "^4.1.18",
		"ts-node": "^10.9.2",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.50.1",
		"vite": "^7.3.0",
		"vitest": "^4.0.16",
		"vitest-sonar-reporter": "^3.0.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"dependencies": {
		"@inlang/paraglide-js": "^2.7.0",
		"@lucide/svelte": "^0.562.0",
		"daisyui": "^5.5.14",
		"fast-xml-parser": "^5.3.3"
	}
}
